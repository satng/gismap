{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": ["./src/code",
            "./src/code/draw",
            "./src/code/methods",
            "./src/code/material",
            "./src/code/paint",
            "./src/code/tools",
            "./src/code/layer",
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "node_modules/minami",
        "destination": "./docs/"
    }
}